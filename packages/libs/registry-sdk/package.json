{
  "name": "@lit-protocol/vincent-registry-sdk",
  "version": "3.3.2",
  "description": "Vincent Registry API definitions and generated code",
  "packageManager": "pnpm@10.7.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "nx build",
    "watch": "pnpm chokidar 'src/lib/**/*' --initial --await-write-finish --delay 1000 --silent -c 'pnpm nx run generate-all'"
  },
  "devDependencies": {
    "@rtk-query/codegen-openapi": "^1.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.5",
    "chokidar-cli": "^3.0.0",
    "ts-node": "^10.9.2"
  },
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "files": [
    "dist/**",
    "openapi.json",
    "*.md",
    "docs/**"
  ],
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.2",
    "@reduxjs/toolkit": "^2.8.2",
    "tslib": "2.8.1",
    "zod": "3.25.64"
  }
}
